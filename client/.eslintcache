) 2020. Mikael Lazarev\n */\n\n\nimport * as app from \"./app/actions\";\nimport * as wallet from \"./wallet/actions\";\nimport * as token from \"./token/actions\";\nimport * as substrate from './substrate/actions'\nimport * as web3 from \"./web3/actions\";\n\nexport default {\n  app,\n  wallet,\n  token,\n  substrate,\n  web3,\n};\n",["300","301"],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/token/actions.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/token/reducer.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AppBarWallet/index.tsx",["302"],"/Users/mikhail/Coding/substrate1/contracts/client/src/screens/SwapScreen/index.tsx",["303","304","305"],"import React, { useEffect, useState } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { DarrContainerS, InputBlockS } from \"./styles\";\nimport { AmountInput } from \"../../components/AmountInput\";\nimport { CurrencySelector } from \"../../components/CurrencySelector\";\nimport { AssetType } from \"../../core/asset\";\nimport { usePool } from \"../../store/pool/hook\";\nimport actions from \"../../store/actions\";\nimport { HBar, RateTitle, StyledCard, VSpace } from \"../../theme\";\nimport { Web3ButtonWrapper } from \"../../components/Buttons\";\nimport { Button100W } from \"../../components/Buttons/styles\";\nimport { useOperation } from \"dlt-operations\";\nimport { useSubstrate } from \"../../store/substrate/hook\";\nimport { useWeb3 } from \"../../store/web3/hook\";\nimport { useAssets } from \"../../store/wallet/hook\";\n\nexport interface SmartFieldProps {}\n\nexport function SwapScreen({}: SmartFieldProps): React.ReactElement {\n  const [target, setTarget] = useState<AssetType>(\"token\");\n  const [source, setSource] = useState<AssetType>(\"eth\");\n  const [amount, setAmount] = useState(0);\n  const [hash, setHash] = useState(\"0\");\n\n  const dispatch = useDispatch();\n  const { api } = useSubstrate();\n  const { provider } = useWeb3();\n\n  useEffect(() => {\n    if (provider) dispatch(actions.token.getTokenInfo());\n    if (api && provider) dispatch(actions.pool.updatePool());\n  }, [api, provider]);\n\n  const { rate } = usePool();\n  const ethAsset = useAssets(\"eth\");\n  const tokenAsset = useAssets(\"token\");\n\n  const anotherAsset = (a: AssetType) => (a === \"eth\" ? \"token\" : \"eth\");\n\n  const onSelect = (a: AssetType, selector: \"src\" | \"trg\") => {\n    setSource(selector === \"src\" ? a : anotherAsset(a));\n    setTarget(selector === \"trg\" ? a : anotherAsset(a));\n  };\n\n  const currentRate = target === \"token\" ? rate : 1 / rate;\n\n  const onSwap = () => {\n    const newHash = Date.now().toString();\n    dispatch(actions.pool.swapAction(target, amount, newHash));\n    setHash(newHash);\n  };\n\n  const operation = useOperation(hash);\n\n  const fromAsset = source === \"eth\" ? ethAsset.symbol : tokenAsset.symbol;\n  const toAsset = target === \"eth\" ? ethAsset.symbol : tokenAsset.symbol;\n\n  const humanRate = `${rate.toFixed(4)} ${fromAsset} / ${toAsset}`;\n  const swapButtonText = `Swap ${fromAsset} to ${toAsset}`;\n\n  useEffect(() => {\n    if (hash !== \"0\") {\n      switch (operation?.status) {\n        case \"STATUS.SUCCESS\":\n          dispatch(\n            actions.modal.show(\n              \"Operation submitted\",\n              `You swap operation from ${source} to ${target} for ${amount} was successfully submitted`\n            )\n          );\n          setAmount(0);\n          setHash(\"0\");\n          break;\n        case \"STATUS.FAILURE\":\n          dispatch(\n            actions.modal.show(\"Error\", operation?.error || \"Unknown error\")\n          );\n          setHash(\"0\");\n          break;\n      }\n    }\n  }, [hash, operation?.status]);\n\n  return (\n    <>\n      <h1>Swap</h1>\n      <HBar>\n        <RateTitle> Rate: {humanRate}</RateTitle>\n      </HBar>\n      <StyledCard>\n        <InputBlockS>\n          <AmountInput\n            title={\"From\"}\n            amount={amount / currentRate}\n            disabled={true}\n          />\n          <CurrencySelector\n            selected={source}\n            onSelect={(a) => onSelect(a, \"src\")}\n          />\n        </InputBlockS>\n      </StyledCard>\n      <DarrContainerS>&darr;</DarrContainerS>\n      <StyledCard>\n        <InputBlockS>\n          <AmountInput title={\"To\"} amount={amount} setAmount={setAmount} />\n          <CurrencySelector\n            selected={target}\n            onSelect={(a) => onSelect(a, \"trg\")}\n          />\n        </InputBlockS>\n      </StyledCard>\n      <VSpace height={15} />\n      <Web3ButtonWrapper>\n        <Button100W onClick={onSwap}>{swapButtonText}</Button100W>\n      </Web3ButtonWrapper>\n    </>\n  );\n}\n","/Users/mikhail/Coding/substrate1/contracts/client/src/screens/WalletScreen/index.tsx",["306","307","308","309"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/TransferCard/index.tsx",["310"],"import React from \"react\";\nimport {AssetTitle, TransferCardContainer} from \"./styles\";\nimport {DepositButtonBar} from \"../DepositButtonBar\";\nimport {AssetType} from \"../../core/asset\";\nimport {useAssets} from \"../../store/wallet/hook\";\nimport {DoubleIndicator} from \"../DoubleIndicator\";\nimport {HBar, VSpace} from \"../../theme\";\nimport {Web3ButtonWrapper} from \"../Buttons\";\n\nexport interface TransferCardProps {\n  asset: AssetType;\n}\n\nexport function TransferCard({ asset }: TransferCardProps): React.ReactElement {\n  const { name, decimals, mainBalance, l2Balance, icon } = useAssets(asset);\n\n   \n\n  return (\n    <TransferCardContainer>\n      <HBar>\n        <img src={icon} height={\"20px\"} />\n        <AssetTitle> {name}</AssetTitle>\n      </HBar>\n      <DoubleIndicator\n        leftTitle={\"Wallet\"}\n        leftValue={mainBalance}\n        leftDecimals={decimals}\n        rightTitle={\"Deposited\"}\n        rightValue={l2Balance}\n        rightDecimals={decimals}\n      />\n      <VSpace height={12} />\n      <Web3ButtonWrapper>\n        <DepositButtonBar asset={asset} />\n      </Web3ButtonWrapper>\n    </TransferCardContainer>\n  );\n}\n","/Users/mikhail/Coding/substrate1/contracts/client/src/components/TransferCard/styles.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/utils/formatter.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/web3/hook.ts",["311"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/DepositButtonBar/index.tsx",["312","313"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/DepositButtonBar/styles.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/SmartNumberInput/index.tsx",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/utils/validate.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/SmartNumberInput/styles.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/token/hook.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AppBarWallet/styles.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/substrate/actions.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/substrate/reducer.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/wallet/hook.ts",["314"],"import { Asset, AssetType } from \"../../core/asset\";\nimport { BigNumber } from \"ethers\";\nimport { useToken } from \"../token/hook\";\nimport { useWeb3 } from \"../web3/hook\";\nimport { useSubstrate } from \"../substrate/hook\";\nimport { useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport actions from \"../actions\";\nimport {ICONS} from \"../../config\";\n\nexport function useAssets(asset: AssetType): Asset {\n  const token = useToken();\n  const web3 = useWeb3();\n  const substrate = useSubstrate();\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (!token.infoLoaded) {\n      dispatch(actions.token.getTokenInfo());\n    }\n  }, [token.infoLoaded]);\n\n  switch (asset) {\n    case \"eth\":\n      return {\n        name: \"Ethereum\",\n        symbol: \"Eth\",\n        address: \"\",\n        decimals: 18,\n        mainBalance: BigNumber.from(web3.balance || 0),\n        l2Balance: BigNumber.from(substrate.ethBalance || 0),\n        allowance: BigNumber.from(0),\n        icon: ICONS[asset]\n      };\n    case \"token\":\n      return {\n        name: token.name,\n        symbol: token.symbol,\n        address: token.address || \"\",\n        decimals: token.decimals,\n        mainBalance: BigNumber.from(token.balance || 0),\n        l2Balance: BigNumber.from(substrate.tokenBalance || 0),\n        allowance: BigNumber.from(token.allowance || 0),\n        icon: ICONS[asset]\n      };\n  }\n}\n","/Users/mikhail/Coding/substrate1/contracts/client/src/store/wallet/actions.ts",[],["315","316"],"/Users/mikhail/Coding/substrate1/contracts/client/src/screens/PoolScreen/index.tsx",["317","318","319"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AmountIndicator/styles.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/pool/reducer.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/DoubleIndicator/index.tsx",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/store/pool/actions.ts",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/LiquidityButtonBar/index.tsx",["320","321"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AmountIndicator/index.tsx",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AmountInput/index.tsx",[],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/CurrencySelector/index.tsx",["322"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/CurrencySelector/styles.ts",["323"],"/Users/mikhail/Coding/substrate1/contracts/client/src/components/AmountInput/styles.ts",[],"/Users/mikha